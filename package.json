    "babel-register": "^6.24.1",
    "chai": "^4.0.2",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1",
    "jest": "^21.2.1",
    "mocha": "^3.4.2"
  },
  "main": "./src/client/index"
