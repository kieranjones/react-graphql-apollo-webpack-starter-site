{
  "name": "react-graphql-apollo-webpack-starter-site",
  "version": "1.0.0",
  "description": "React, GraphQL, Apollo, Webpack Starter Site",
  "engines": {
    "node": "8.3.0",
    "npm": "5.3.0"
  },
  "scripts": {
    "start": "npm run build && node src/server/index.js",
    "dev:start": "webpack --config ./webpack.dev.js --display-error-details --progress --colors --display-error-details --progress --colors && node src/server/index.js",
    "build": "webpack --config ./webpack.prod.js",
    "test": "nyc --reporter=text mocha --require babel-polyfill --require babel-register --require ignore-styles --require test/setup.js --recursive --check-leaks",
    "lint": "node ./node_modules/eslint/bin/eslint.js --fix --ext .js,.jsx src/"
  },
  "dependencies": {
